{
  "containerDefinitions": [
    {
      "name": "designprototypes",
      "image": "{{IMAGE}}",
      "cpu": 10,
      "memory": 256,
      "essential": true,
      "command": [],
      "portMappings": [
        {
          "hostPort": 81,
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "designprototypes",
          "awslogs-region": "eu-west-1"
        }
      }
    }
  ],
  "family": "designprototypes"
}
